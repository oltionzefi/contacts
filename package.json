  "private": true,
  "dependencies": {
    "@datorama/akita": "^4.14.1",
    "@types/jest": "^24.0.25",
    "@types/node": "12.12.11",
    "@types/react": "16.9.12",
    "@types/react-dom": "16.9.4",
    "formik": "^2.0.6",
    "i18next": "^19.0.1",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-fetch-backend": "^2.2.0",
    "office-ui-fabric-react": "^7.64.1",
    "react": "^16.12.0",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.12.0",
    "react-i18next": "^11.2.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "rxjs": "^6.5.3",
    "typescript": "3.7.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "yarn test --coverage",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "type-check": "tsc --noEmit",
    "eject": "react-scripts eject",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,tsx,ts,json}\"",
    "translations-scan": "i18next-scanner --config i18next-scanner.config.js",
    "serve:build": "react-scripts build && serve -s build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.0",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/react-router-dom": "^5.1.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-dev-expression": "^0.2.2",
    "babel-plugin-macros": "^2.8.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "husky": "^3.1.0",
    "i18next-scanner": "^2.10.3",
    "i18next-scanner-typescript": "^1.0.3",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "open-cli": "^5.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "serve": "^11.3.0",
    "ts-jest": "^24.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
