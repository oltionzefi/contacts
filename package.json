    "i18next-scanner": "^3.0.0",
    "i18next-scanner-typescript": "^1.0.6",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "open-cli": "^6.0.1",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "release-it": "^14.3.0",
    "serve": "^11.3.2",
    "ts-jest": "^26.5.1"
